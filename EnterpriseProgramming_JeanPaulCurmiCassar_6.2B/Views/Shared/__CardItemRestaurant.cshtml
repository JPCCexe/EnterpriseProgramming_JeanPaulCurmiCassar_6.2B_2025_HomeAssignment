@model Domain.Models.Restaurant

<div class="card">
    <div class="card-body">
        <h5 class="card-title">@Model.Name</h5>
        <p class="card-text">
            <strong>Owner:</strong> @Model.OwnerEmailAddress<br />
            <strong>Phone:</strong> @Model.Phone<br />
            <strong>Status:</strong> @Model.Status
        </p>
    </div>
</div>